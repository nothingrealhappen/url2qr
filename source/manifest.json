{
	"name": "URL2QR",
	"version": "1.0",
	"manifest_version": 2,
	"description": "将当前URL的地址转成二维码",
  "author": "Vanilla",
  "homepage_url": "http://liyaodong.com",
	"icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "tabs",
    "storage"
  ],
  "options_page": "options.html"
}
